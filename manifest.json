{
  "manifest_version": 2,

  "name": "RewardsDen Testing",
  "description": "Loads RewardsDen code onto a publisher's site for testing.",
  "version": "0.1",

  "permissions": [
    "tabs",
    "<all_urls>"
  ],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
